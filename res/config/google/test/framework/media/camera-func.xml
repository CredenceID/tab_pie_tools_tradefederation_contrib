<configuration description="Runs camera functional tests">
    <option name="test-tag" value="camera_functional_test" />

    <target_preparer class="com.android.tradefed.targetprep.TestAppInstallSetup">
        <option name="test-file-name" value="mediaframeworktest.apk" />
    </target_preparer>

    <test class="com.android.tradefed.testtype.InstrumentationTest">
        <option name="class" value="com.android.mediaframeworktest.functional.camera.CameraFunctionalTest" />
        <option name="package" value="com.android.mediaframeworktest" />
        <option name="runner" value="com.android.mediaframeworktest.CameraStressTestRunner" />
        <option name="run-name" value="camera_functional_test" />
    </test>
</configuration>
